{
    "id": "#main",
    "class": "CommandLineTool",
    "requirements": [
        {
            "class": "DockerRequirement",
            "dockerPull": "ubuntu:latest"
        }
    ],
    "baseCommand": "echo",
    "inputs": [
        {
            "type": "string",
            "inputBinding": {
                "position": 1
            },
            "id": "#main/message"
        }
    ],
    "stdout": "hello-stdout.txt",
    "hints": [
        {
            "loadListing": "deep_listing",
            "class": "LoadListingRequirement"
        },
        {
            "networkAccess": true,
            "class": "NetworkAccess"
        }
    ],
    "outputs": [
        {
            "id": "#main/salutation",
            "type": "string",
            "outputBinding": {
                "glob": "hello-stdout.txt",
                "loadContents": true,
                "outputEval": "$(self[0].contents.trim())"
            }
        }
    ],
    "cwlVersion": "v1.2"
}