{
  "$graph": [
    {
      "baseCommand": [
        "echo"
      ],
      "class": "CommandLineTool",
      "id": "#bar.cwl",
      "inputs": [
        {
          "id": "#bar.cwl/in1",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#bar.cwl/out1",
          "outputBinding": {
            "outputEval": "bar $(inputs.in1)"
          },
          "type": "string"
        }
      ]
    },
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "id": "#main/in1",
          "type": {
            "items": "int",
            "type": "array"
          }
        }
      ],
      "outputs": [
        {
          "id": "#main/out1",
          "linkMerge": "merge_flattened",
          "outputSource": [
            "#main/step1/out1",
            "#main/step2/out1"
          ],
          "pickValue": "all_non_null",
          "type": {
            "items": "string",
            "type": "array"
          }
        }
      ],
      "requirements": [
        {
          "class": "InlineJavascriptRequirement"
        },
        {
          "class": "MultipleInputFeatureRequirement"
        },
        {
          "class": "ScatterFeatureRequirement"
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/in1",
              "source": "#main/in1"
            }
          ],
          "out": [
            "#main/step1/out1"
          ],
          "run": "#foo.cwl",
          "scatter": "#main/step1/in1",
          "when": "$(inputs.in1 % 2 == 0)"
        },
        {
          "id": "#main/step2",
          "in": [
            {
              "id": "#main/step2/in1",
              "source": "#main/in1"
            }
          ],
          "out": [
            "#main/step2/out1"
          ],
          "run": "#bar.cwl",
          "scatter": "#main/step2/in1",
          "when": "$(inputs.in1 % 2 == 1)"
        }
      ]
    },
    {
      "baseCommand": [
        "echo"
      ],
      "class": "CommandLineTool",
      "id": "#foo.cwl",
      "inputs": [
        {
          "id": "#foo.cwl/in1",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#foo.cwl/out1",
          "outputBinding": {
            "outputEval": "foo $(inputs.in1)"
          },
          "type": "string"
        }
      ]
    }
  ],
  "cwlVersion": "v1.2"
}
