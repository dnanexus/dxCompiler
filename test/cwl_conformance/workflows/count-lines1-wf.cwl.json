{
  "$graph": [
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "id": "#main/file1",
          "type": "File"
        }
      ],
      "outputs": [
        {
          "id": "#main/count_output",
          "outputSource": "#main/step2/output",
          "type": "int"
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/file1",
              "source": "#main/file1"
            }
          ],
          "out": [
            "#main/step1/output"
          ],
          "run": "#wc-tool.cwl"
        },
        {
          "id": "#main/step2",
          "in": [
            {
              "id": "#main/step2/file1",
              "source": "#main/step1/output"
            }
          ],
          "out": [
            "#main/step2/output"
          ],
          "run": "#parseInt-tool.cwl"
        }
      ]
    },
    {
      "class": "ExpressionTool",
      "expression": "$({'output': parseInt(inputs.file1.contents)})",
      "id": "#parseInt-tool.cwl",
      "inputs": [
        {
          "id": "#parseInt-tool.cwl/file1",
          "loadContents": true,
          "type": "File"
        }
      ],
      "outputs": [
        {
          "id": "#parseInt-tool.cwl/output",
          "type": "int"
        }
      ],
      "requirements": [
        {
          "class": "InlineJavascriptRequirement"
        }
      ]
    },
    {
      "baseCommand": [
        "wc",
        "-l"
      ],
      "class": "CommandLineTool",
      "id": "#wc-tool.cwl",
      "inputs": [
        {
          "id": "#wc-tool.cwl/file1",
          "type": "File"
        }
      ],
      "outputs": [
        {
          "id": "#wc-tool.cwl/output",
          "outputBinding": {
            "glob": "output"
          },
          "type": "File"
        }
      ],
      "stdin": "$(inputs.file1.path)",
      "stdout": "output"
    }
  ],
  "cwlVersion": "v1.2"
}
