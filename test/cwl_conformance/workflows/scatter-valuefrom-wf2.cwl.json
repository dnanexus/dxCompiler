{
  "class": "Workflow",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/inp1",
      "type": {
        "items": {
          "fields": [
            {
              "name": "#main/inp1/instr/instr",
              "type": "string"
            }
          ],
          "name": "#main/inp1/instr",
          "type": "record"
        },
        "type": "array"
      }
    },
    {
      "id": "#main/inp2",
      "type": {
        "items": "string",
        "type": "array"
      }
    }
  ],
  "outputs": [
    {
      "id": "#main/out",
      "outputSource": "#main/step1/echo_out",
      "type": {
        "items": {
          "items": "string",
          "type": "array"
        },
        "type": "array"
      }
    }
  ],
  "requirements": [
    {
      "class": "ScatterFeatureRequirement"
    },
    {
      "class": "StepInputExpressionRequirement"
    }
  ],
  "steps": [
    {
      "id": "#main/step1",
      "in": [
        {
          "id": "#main/step1/echo_in1",
          "source": "#main/inp1",
          "valueFrom": "$(self.instr)"
        },
        {
          "id": "#main/step1/echo_in2",
          "source": "#main/inp2"
        },
        {
          "id": "#main/step1/first",
          "source": "#main/inp1",
          "valueFrom": "$(self[0].instr)"
        }
      ],
      "out": [
        "#main/step1/echo_out"
      ],
      "run": {
        "arguments": [
          "-n",
          "foo"
        ],
        "baseCommand": "echo",
        "class": "CommandLineTool",
        "id": "#main/step1/run/step1command",
        "inputs": [
          {
            "id": "#main/step1/run/step1command/echo_in1",
            "inputBinding": {
              "position": 2
            },
            "type": "string"
          },
          {
            "id": "#main/step1/run/step1command/echo_in2",
            "inputBinding": {
              "position": 3
            },
            "type": "string"
          },
          {
            "id": "#main/step1/run/step1command/first",
            "inputBinding": {
              "position": 1
            },
            "type": "string"
          }
        ],
        "outputs": [
          {
            "id": "#main/step1/run/step1command/echo_out",
            "outputBinding": {
              "glob": "step1_out",
              "loadContents": true,
              "outputEval": "$(self[0].contents)"
            },
            "type": "string"
          }
        ],
        "stdout": "step1_out"
      },
      "scatter": [
        "#main/step1/echo_in1",
        "#main/step1/echo_in2"
      ],
      "scatterMethod": "nested_crossproduct"
    }
  ]
}
