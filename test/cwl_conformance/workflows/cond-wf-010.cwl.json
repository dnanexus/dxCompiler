{
  "$graph": [
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "id": "#main/val",
          "type": {
            "items": "int",
            "type": "array"
          }
        }
      ],
      "outputs": [
        {
          "id": "#main/out1",
          "outputSource": "#main/step1/out1",
          "pickValue": "all_non_null",
          "type": {
            "items": "string",
            "type": "array"
          }
        }
      ],
      "requirements": [
        {
          "class": "InlineJavascriptRequirement"
        },
        {
          "class": "ScatterFeatureRequirement"
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/a_new_var",
              "source": "#main/val"
            },
            {
              "id": "#main/step1/in1",
              "source": "#main/val"
            }
          ],
          "out": [
            "#main/step1/out1"
          ],
          "run": "#foo.cwl",
          "scatter": [
            "#main/step1/in1",
            "#main/step1/a_new_var"
          ],
          "scatterMethod": "dotproduct",
          "when": "$(inputs.a_new_var > 3)"
        }
      ]
    },
    {
      "baseCommand": [
        "echo"
      ],
      "class": "CommandLineTool",
      "id": "#foo.cwl",
      "inputs": [
        {
          "id": "#foo.cwl/in1",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#foo.cwl/out1",
          "outputBinding": {
            "outputEval": "foo $(inputs.in1)"
          },
          "type": "string"
        }
      ]
    }
  ],
  "cwlVersion": "v1.2"
}
