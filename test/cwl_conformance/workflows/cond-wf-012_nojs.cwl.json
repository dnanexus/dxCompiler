{
  "$graph": [
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "default": 1,
          "id": "#main/test",
          "type": "int"
        },
        {
          "default": 23,
          "id": "#main/val",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#main/out1",
          "outputSource": "#main/step1/out1",
          "type": [
            "null",
            "string"
          ]
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/a_new_var",
              "source": "#main/test"
            },
            {
              "id": "#main/step1/in1",
              "source": "#main/val"
            }
          ],
          "out": [
            "#main/step1/out1"
          ],
          "run": "#foo.cwl",
          "when": "$(inputs.a_new_var)"
        }
      ]
    },
    {
      "baseCommand": [
        "echo"
      ],
      "class": "CommandLineTool",
      "id": "#foo.cwl",
      "inputs": [
        {
          "id": "#foo.cwl/in1",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#foo.cwl/out1",
          "outputBinding": {
            "outputEval": "foo $(inputs.in1)"
          },
          "type": "string"
        }
      ]
    }
  ],
  "cwlVersion": "v1.2"
}
