{
  "class": "Workflow",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/a",
      "type": "int"
    },
    {
      "id": "#main/b",
      "type": "int"
    }
  ],
  "outputs": [
    {
      "id": "#main/val",
      "outputSource": "#main/step1/echo_out",
      "type": "string"
    }
  ],
  "requirements": [
    {
      "class": "StepInputExpressionRequirement"
    },
    {
      "class": "InlineJavascriptRequirement"
    }
  ],
  "steps": [
    {
      "id": "#main/step1",
      "in": [
        {
          "id": "#main/step1/a",
          "source": "#main/a"
        },
        {
          "id": "#main/step1/b",
          "source": "#main/b"
        },
        {
          "id": "#main/step1/c",
          "valueFrom": "$(inputs.a + inputs.b)"
        }
      ],
      "out": [
        "#main/step1/echo_out"
      ],
      "run": {
        "baseCommand": "echo",
        "class": "CommandLineTool",
        "id": "#main/step1/run/echo",
        "inputs": [
          {
            "id": "#main/step1/run/echo/c",
            "inputBinding": {},
            "type": "int"
          }
        ],
        "outputs": [
          {
            "id": "#main/step1/run/echo/echo_out",
            "outputBinding": {
              "glob": "step1_out",
              "loadContents": true,
              "outputEval": "$(self[0].contents)"
            },
            "type": "string"
          }
        ],
        "stdout": "step1_out"
      }
    }
  ]
}
