{
  "$graph": [
    {
      "baseCommand": "echo",
      "class": "CommandLineTool",
      "id": "#echo-tool.cwl",
      "inputs": [
        {
          "id": "#echo-tool.cwl/in",
          "inputBinding": {},
          "type": "Any"
        }
      ],
      "outputs": [
        {
          "id": "#echo-tool.cwl/out",
          "outputBinding": {
            "glob": "out.txt",
            "loadContents": true,
            "outputEval": "$(self[0].contents)"
          },
          "type": "string"
        }
      ],
      "stdout": "out.txt"
    },
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "default": "hello inp1",
          "id": "#main/inp1",
          "type": "string"
        },
        {
          "default": "hello inp2",
          "id": "#main/inp2",
          "type": "string"
        }
      ],
      "outputs": [
        {
          "id": "#main/out",
          "outputSource": "#main/step1/out",
          "type": "string"
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/in",
              "source": "#main/inp1"
            },
            {
              "id": "#main/step1/in2",
              "source": "#main/inp2"
            }
          ],
          "out": [
            "#main/step1/out"
          ],
          "run": "#echo-tool.cwl"
        }
      ]
    }
  ],
  "cwlVersion": "v1.2"
}
