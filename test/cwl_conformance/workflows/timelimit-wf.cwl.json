{
  "class": "Workflow",
  "cwlVersion": "v1.2",
  "inputs": [
    {
      "id": "i",
      "type": [
        "null",
        "int"
      ]
    }
  ],
  "outputs": [
    {
      "id": "o",
      "outputSource": "step1/o",
      "type": [
        "null",
        "string"
      ]
    }
  ],
  "requirements": [
    {
      "class": "ToolTimeLimit",
      "timelimit": 3
    },
    {
      "class": "SubworkflowFeatureRequirement"
    },
    {
      "class": "InlineJavascriptRequirement"
    }
  ],
  "steps": [
    {
      "id": "step1",
      "in": [
        {
          "id": "i",
          "source": "i"
        }
      ],
      "out": [
        "o"
      ],
      "run": {
        "baseCommand": [
          "sleep",
          "10"
        ],
        "class": "CommandLineTool",
        "inputs": [
          {
            "id": "i",
            "type": [
              "null",
              "int"
            ]
          }
        ],
        "outputs": [
          {
            "id": "o",
            "outputBinding": {
              "outputEval": "time passed"
            },
            "type": [
              "null",
              "string"
            ]
          }
        ],
        "requirements": [
          {
            "class": "InlineJavascriptRequirement"
          }
        ]
      }
    }
  ]
}
