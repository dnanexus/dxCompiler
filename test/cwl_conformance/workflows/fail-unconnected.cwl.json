{
    "$graph": [
        {
            "class": "Workflow",
            "inputs": [
                {
                    "type": "string",
                    "default": "hello inp1",
                    "id": "#main/inp1"
                },
                {
                    "type": "string",
                    "default": "hello inp2",
                    "id": "#main/inp2"
                }
            ],
            "outputs": [
                {
                    "type": "string",
                    "outputSource": "#main/step1/out",
                    "id": "#main/out"
                }
            ],
            "steps": [
                {
                    "in": [
                        {
                            "source": "#main/inp1",
                            "id": "#main/step1/in"
                        },
                        {
                            "source": "#main/inp2",
                            "id": "#main/step1/in2"
                        }
                    ],
                    "out": [
                        "#main/step1/out"
                    ],
                    "run": "#fail-unspecified-input.cwl",
                    "id": "#main/step1"
                }
            ],
            "id": "#main"
        },
        {
            "class": "CommandLineTool",
            "inputs": [
                {
                    "type": "string",
                    "id": "#fail-unspecified-input.cwl/in"
                }
            ],
            "outputs": [
                {
                    "type": "string",
                    "outputBinding": {
                        "glob": "out.txt",
                        "loadContents": true,
                        "outputEval": "$(self[0].contents)"
                    },
                    "id": "#fail-unspecified-input.cwl/out"
                }
            ],
            "stdout": "out.txt",
            "arguments": [
                "echo",
                "$(inputs.in)",
                "$(inputs.in2)"
            ],
            "id": "#fail-unspecified-input.cwl"
        }
    ],
    "cwlVersion": "v1.2"
}