{
  "class": "Workflow",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [],
  "outputs": [
    {
      "id": "#main/val",
      "outputSource": "#main/step1/echo_out",
      "type": "string"
    }
  ],
  "requirements": [
    {
      "class": "StepInputExpressionRequirement"
    }
  ],
  "steps": [
    {
      "id": "#main/step1",
      "in": [
        {
          "id": "#main/step1/a",
          "valueFrom": "moocow"
        }
      ],
      "out": [
        "#main/step1/echo_out"
      ],
      "run": {
        "baseCommand": "echo",
        "class": "CommandLineTool",
        "id": "#main/step1/run/echo",
        "inputs": [
          {
            "id": "#main/step1/run/echo/a",
            "inputBinding": {},
            "type": "string"
          }
        ],
        "outputs": [
          {
            "id": "#main/step1/run/echo/echo_out",
            "outputBinding": {
              "glob": "step1_out",
              "loadContents": true,
              "outputEval": "$(self[0].contents)"
            },
            "type": "string"
          }
        ],
        "stdout": "step1_out"
      }
    }
  ]
}
