{
  "$graph": [
    {
      "baseCommand": [
        "/bin/sh",
        "-c",
        "echo $TEST_ENV"
      ],
      "class": "CommandLineTool",
      "id": "#env-tool3.cwl",
      "inputs": [
        {
          "id": "#env-tool3.cwl/in",
          "type": "string"
        }
      ],
      "outputs": [
        {
          "id": "#env-tool3.cwl/out",
          "outputBinding": {
            "glob": "out"
          },
          "type": "File"
        }
      ],
      "requirements": [
        {
          "class": "EnvVarRequirement",
          "envDef": [
            {
              "envName": "TEST_ENV",
              "envValue": "$(inputs.in)"
            }
          ]
        }
      ],
      "stdout": "out"
    },
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "id": "#main/in",
          "type": "string"
        }
      ],
      "outputs": [
        {
          "id": "#main/out",
          "outputSource": "#main/step1/out",
          "type": "File"
        }
      ],
      "requirements": [
        {
          "class": "EnvVarRequirement",
          "envDef": [
            {
              "envName": "TEST_ENV",
              "envValue": "override"
            }
          ]
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/in",
              "source": "#main/in"
            }
          ],
          "out": [
            "#main/step1/out"
          ],
          "run": "#env-tool3.cwl"
        }
      ]
    }
  ],
  "cwlVersion": "v1.2"
}
