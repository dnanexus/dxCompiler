{
  "$graph": [
    {
      "baseCommand": [
        "echo"
      ],
      "class": "CommandLineTool",
      "id": "#bar.cwl",
      "inputs": [
        {
          "id": "#bar.cwl/in1",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#bar.cwl/out1",
          "outputBinding": {
            "outputEval": "bar $(inputs.in1)"
          },
          "type": "string"
        }
      ]
    },
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "default": [
            1,
            2,
            3,
            4,
            5,
            6
          ],
          "id": "#main/in1",
          "type": {
            "items": "int",
            "type": "array"
          }
        },
        {
          "default": [
            false,
            true,
            false,
            true,
            false,
            true
          ],
          "id": "#main/test1",
          "type": {
            "items": "boolean",
            "type": "array"
          }
        },
        {
          "default": [
            true,
            false,
            true,
            false,
            true,
            false
          ],
          "id": "#main/test2",
          "type": {
            "items": "boolean",
            "type": "array"
          }
        }
      ],
      "outputs": [
        {
          "id": "#main/out1",
          "linkMerge": "merge_flattened",
          "outputSource": [
            "#main/step1/out1",
            "#main/step2/out1"
          ],
          "pickValue": "all_non_null",
          "type": {
            "items": "string",
            "type": "array"
          }
        }
      ],
      "requirements": [
        {
          "class": "MultipleInputFeatureRequirement"
        },
        {
          "class": "ScatterFeatureRequirement"
        }
      ],
      "steps": [
        {
          "id": "#main/step1",
          "in": [
            {
              "id": "#main/step1/another_var",
              "source": "#main/test1"
            },
            {
              "id": "#main/step1/in1",
              "source": "#main/in1"
            }
          ],
          "out": [
            "#main/step1/out1"
          ],
          "run": "#foo.cwl",
          "scatter": [
            "#main/step1/in1",
            "#main/step1/another_var"
          ],
          "scatterMethod": "dotproduct",
          "when": "$(inputs.another_var)"
        },
        {
          "id": "#main/step2",
          "in": [
            {
              "id": "#main/step2/another_var",
              "source": "#main/test2"
            },
            {
              "id": "#main/step2/in1",
              "source": "#main/in1"
            }
          ],
          "out": [
            "#main/step2/out1"
          ],
          "run": "#bar.cwl",
          "scatter": [
            "#main/step2/in1",
            "#main/step2/another_var"
          ],
          "scatterMethod": "dotproduct",
          "when": "$(inputs.another_var)"
        }
      ]
    },
    {
      "baseCommand": [
        "echo"
      ],
      "class": "CommandLineTool",
      "id": "#foo.cwl",
      "inputs": [
        {
          "id": "#foo.cwl/in1",
          "type": "int"
        }
      ],
      "outputs": [
        {
          "id": "#foo.cwl/out1",
          "outputBinding": {
            "outputEval": "foo $(inputs.in1)"
          },
          "type": "string"
        }
      ]
    }
  ],
  "cwlVersion": "v1.2"
}
