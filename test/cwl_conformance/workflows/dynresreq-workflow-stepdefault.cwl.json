{
  "$graph": [
    {
      "baseCommand": [
        "cat"
      ],
      "class": "CommandLineTool",
      "id": "#cat-tool.cwl",
      "inputs": [
        {
          "id": "#cat-tool.cwl/file1",
          "type": "File"
        }
      ],
      "outputs": [
        {
          "id": "#cat-tool.cwl/output",
          "outputBinding": {
            "glob": "output"
          },
          "type": "File"
        }
      ],
      "stdin": "$(inputs.file1.path)",
      "stdout": "output"
    },
    {
      "class": "Workflow",
      "id": "#main",
      "inputs": [
        {
          "id": "#main/special_file",
          "type": [
            "null",
            "File"
          ]
        }
      ],
      "outputs": [
        {
          "id": "#main/cores",
          "outputSource": "#main/report/output",
          "type": "File"
        }
      ],
      "steps": [
        {
          "id": "#main/count",
          "in": [
            {
              "default": {
                "class": "File",
                "location": "dx://file-G1353z00yzZYkb2V4j4g7yb9"
              },
              "id": "#main/count/special_file",
              "source": "#main/special_file"
            }
          ],
          "out": [
            "#main/count/output"
          ],
          "run": "#dynresreq.cwl"
        },
        {
          "id": "#main/report",
          "in": [
            {
              "id": "#main/report/file1",
              "source": "#main/count/output"
            }
          ],
          "out": [
            "#main/report/output"
          ],
          "run": "#cat-tool.cwl"
        }
      ]
    },
    {
      "arguments": [
        "$(runtime.cores)"
      ],
      "baseCommand": "echo",
      "class": "CommandLineTool",
      "id": "#dynresreq.cwl",
      "inputs": [
        {
          "id": "#dynresreq.cwl/special_file",
          "type": "File"
        }
      ],
      "outputs": [
        {
          "id": "#dynresreq.cwl/output",
          "outputBinding": {
            "glob": "cores.txt"
          },
          "type": "File"
        }
      ],
      "requirements": [
        {
          "class": "ResourceRequirement",
          "coresMax": "$(inputs.special_file.size)",
          "coresMin": "$(inputs.special_file.size)"
        }
      ],
      "stdout": "cores.txt"
    }
  ],
  "cwlVersion": "v1.2"
}
