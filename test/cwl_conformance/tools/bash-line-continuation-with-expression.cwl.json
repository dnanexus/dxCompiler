{
  "baseCommand": [
    "bash",
    "script.sh"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "default": "liberty",
      "id": "#main/liberty",
      "type": [
        "null",
        "string"
      ]
    }
  ],
  "outputs": [
    {
      "id": "#main/out",
      "outputBinding": {
        "glob": "out.txt"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        {
          "entry": "echo \\\n  \"My country, 'tis of thee,\" \\\n  \"Sweet land of $(inputs.liberty)\"\n",
          "entryname": "script.sh"
        }
      ]
    }
  ],
  "stdout": "out.txt"
}
