{
  "baseCommand": "cat",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/dir1",
      "inputBinding": {
        "valueFrom": "$(self.listing[0].path)"
      },
      "type": "Directory"
    }
  ],
  "outputs": [
    {
      "id": "#main/output_file",
      "outputBinding": {
        "glob": "output.txt"
      },
      "type": "File"
    }
  ],
  "stdout": "output.txt"
}
