{
  "baseCommand": "true",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/filelist",
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/filelist",
      "outputBinding": {
        "glob": "$(inputs.filelist.basename)"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        "$(inputs.filelist)"
      ]
    }
  ]
}
