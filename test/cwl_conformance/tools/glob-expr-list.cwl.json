{
  "baseCommand": "touch",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/ids",
      "inputBinding": {
        "position": 1
      },
      "type": {
        "items": "string",
        "type": "array"
      }
    }
  ],
  "outputs": [
    {
      "id": "#main/files",
      "outputBinding": {
        "glob": "$(inputs.ids)"
      },
      "type": {
        "items": "File",
        "type": "array"
      }
    }
  ]
}
