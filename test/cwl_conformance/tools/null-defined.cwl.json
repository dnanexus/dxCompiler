{
  "arguments": [
    "echo",
    "$(inputs.file1 === null ? \"t\" : \"f\")"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "inputs": [
    {
      "id": "file1",
      "type": [
        "null",
        "File"
      ]
    }
  ],
  "outputs": [
    {
      "id": "out",
      "outputBinding": {
        "glob": "out.txt",
        "loadContents": true,
        "outputEval": "$(self[0].contents)"
      },
      "type": "string"
    }
  ],
  "requirements": [
    {
      "class": "InlineJavascriptRequirement"
    }
  ],
  "stdout": "out.txt"
}
