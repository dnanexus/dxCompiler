{
  "arguments": [
    "-c",
    "f = open(\"bob.txt\", \"r+\")\nf.seek(8)\nf.write(\"Bob.    \")\nf.close()\n"
  ],
  "baseCommand": "python",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "hints": [
    {
      "class": "DockerRequirement",
      "dockerPull": "python:2-slim"
    }
  ],
  "id": "#main",
  "inputs": [
    {
      "id": "#main/infile",
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/outfile",
      "outputBinding": {
        "glob": "bob.txt"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        {
          "entry": "$(inputs.infile)",
          "entryname": "bob.txt",
          "writable": true
        }
      ]
    }
  ]
}
