{
  "baseCommand": "echo",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "echo-tool.cwl",
  "inputs": [
    {
      "id": "in",
      "inputBinding": {},
      "type": "Any"
    }
  ],
  "outputs": [
    {
      "id": "out",
      "outputBinding": {
        "glob": "out.txt",
        "loadContents": true,
        "outputEval": "$(self[0].contents)"
      },
      "type": "string"
    }
  ],
  "requirements": [
    {
      "class": "InlineJavascriptRequirement"
    }
  ],
  "stdout": "out.txt"
}
