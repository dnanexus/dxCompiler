{
  "baseCommand": [
    "cat"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/file1",
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/output",
      "outputBinding": {
        "glob": "output"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "DockerRequirement",
      "dockerPull": "debian:stretch-slim"
    }
  ],
  "stdin": "$(inputs.file1.path)",
  "stdout": "output"
}
