{
  "baseCommand": [
    "/bin/bash",
    "-c",
    "echo $TEST_ENV"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/in",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "id": "#main/out",
      "outputBinding": {
        "glob": "out"
      },
      "type": "File"
    }
  ],
  "stdout": "out"
}
