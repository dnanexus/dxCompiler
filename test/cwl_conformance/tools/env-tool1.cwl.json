{
  "baseCommand": [
    "/bin/sh",
    "-c",
    "echo $TEST_ENV"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "env-tool1",
  "inputs": [
    {
      "id": "in",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "id": "out",
      "outputBinding": {
        "glob": "out"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "EnvVarRequirement",
      "envDef": {
        "TEST_ENV": "$(inputs.in)"
      }
    },
    {
      "class": "InlineJavascriptRequirement"
    }
  ],
  "stdout": "out"
}
