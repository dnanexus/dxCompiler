{
  "arguments": [
    "$(runtime.cores)"
  ],
  "baseCommand": "echo",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [],
  "outputs": [
    {
      "id": "#main/output",
      "outputBinding": {
        "glob": "cores.txt"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "ResourceRequirement",
      "coresMax": 1.75,
      "coresMin": 1.25
    }
  ],
  "stdout": "cores.txt"
}
