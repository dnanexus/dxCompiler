{
  "baseCommand": [
    "/bin/sh",
    "-c",
    "echo $TEST_ENV"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "hints": [
    {
      "class": "EnvVarRequirement",
      "envDef": [
        {
          "envName": "TEST_ENV",
          "envValue": "hello test env"
        }
      ]
    }
  ],
  "id": "#main",
  "inputs": [],
  "outputs": [
    {
      "id": "#main/out",
      "outputBinding": {
        "glob": "out"
      },
      "type": "File"
    }
  ],
  "stdout": "out"
}
