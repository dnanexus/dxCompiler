{
  "arguments": [
    {
      "shellQuote": false,
      "valueFrom": "&&"
    },
    "find",
    ".",
    {
      "shellQuote": false,
      "valueFrom": "|"
    },
    "sort"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/indir",
      "inputBinding": {
        "position": -1,
        "prefix": "cd"
      },
      "type": "Directory"
    }
  ],
  "outputs": [
    {
      "id": "#main/outlist",
      "outputBinding": {
        "glob": "output.txt"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "ShellCommandRequirement"
    }
  ],
  "stdout": "output.txt"
}
