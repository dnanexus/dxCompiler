{
  "baseCommand": [
    "ls"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/input_list",
      "type": {
        "items": "Directory",
        "type": "array"
      }
    }
  ],
  "label": "stage-array-dirs.cwl",
  "outputs": [
    {
      "id": "#main/output",
      "outputBinding": {
        "glob": [
          "testdir/a",
          "rec/B"
        ]
      },
      "type": {
        "items": "File",
        "type": "array"
      }
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        "$(inputs.input_list)"
      ]
    }
  ]
}
