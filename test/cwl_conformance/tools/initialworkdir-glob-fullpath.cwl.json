{
  "arguments": [
    {
      "position": 0,
      "valueFrom": "$(inputs.INPUT.basename).fai"
    }
  ],
  "baseCommand": [
    "touch"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/INPUT",
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/OUTPUT",
      "outputBinding": {
        "glob": "$(runtime.outdir)/$(inputs.INPUT.basename)"
      },
      "secondaryFiles": [
        {
          "pattern": ".fai",
          "required": null
        }
      ],
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        "$(inputs.INPUT)"
      ]
    }
  ]
}
