{
  "baseCommand": "true",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/newname",
      "type": "string"
    },
    {
      "id": "#main/srcfile",
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/outfile",
      "outputBinding": {
        "glob": "$(inputs.newname)"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        {
          "entry": "$(inputs.srcfile)",
          "entryname": "$(inputs.newname)"
        }
      ]
    }
  ]
}
