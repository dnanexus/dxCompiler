{
  "baseCommand": "echo",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/letters",
      "inputBinding": {
        "position": 1
      },
      "type": {
        "items": {
          "items": "string",
          "type": "array"
        },
        "type": "array"
      }
    }
  ],
  "outputs": [
    {
      "id": "#main/echo",
      "outputBinding": {
        "glob": "echo.txt"
      },
      "type": "File"
    }
  ],
  "stdout": "echo.txt"
}
