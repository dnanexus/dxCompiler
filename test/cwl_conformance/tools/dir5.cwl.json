{
  "arguments": [
    "find",
    "-L",
    ".",
    "!",
    "-path",
    "*.txt",
    {
      "shellQuote": false,
      "valueFrom": "|"
    },
    "sort"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/indir",
      "loadListing": "shallow_listing",
      "type": "Directory"
    }
  ],
  "outputs": [
    {
      "id": "#main/outlist",
      "outputBinding": {
        "glob": "output.txt"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "ShellCommandRequirement"
    },
    {
      "class": "InitialWorkDirRequirement",
      "listing": "$(inputs.indir.listing)"
    }
  ],
  "stdout": "output.txt"
}
