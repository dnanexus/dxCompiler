{
  "baseCommand": [
    "cat",
    "example.conf"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/message",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "id": "#main/out",
      "outputBinding": {
        "glob": "example.conf"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        {
          "entry": "CONFIGVAR=$(inputs.message)\n",
          "entryname": "example.conf"
        }
      ]
    }
  ]
}
