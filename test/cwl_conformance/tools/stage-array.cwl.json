{
  "arguments": [
    {
      "shellQuote": false,
      "valueFrom": "ls | grep -v lsout"
    }
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/input_file",
      "type": "File"
    },
    {
      "id": "#main/optional_file",
      "type": [
        "null",
        "File"
      ]
    },
    {
      "id": "#main/input_list",
      "secondaryFiles": [
        {
          "pattern": "^.tar",
          "required": null
        }
      ],
      "type": {
        "items": "File",
        "type": "array"
      }
    }
  ],
  "label": "stage-array.cwl",
  "outputs": [
    {
      "id": "#main/output",
      "outputBinding": {
        "glob": "lsout"
      },
      "type": [
        "null",
        "File"
      ]
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        "$(inputs.input_file)",
        "$(inputs.optional_file)",
        {
          "entry": "$(inputs.input_list)"
        },
        {
          "entry": "$(null)"
        },
        {
          "entry": "b",
          "entryname": "a"
        }
      ]
    },
    {
      "class": "ShellCommandRequirement"
    }
  ],
  "stdout": "lsout"
}
