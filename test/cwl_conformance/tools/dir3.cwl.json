{
  "baseCommand": [
    "tar",
    "xvf"
  ],
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/inf",
      "inputBinding": {
        "position": 1
      },
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/outdir",
      "outputBinding": {
        "glob": "."
      },
      "type": "Directory"
    }
  ]
}
