{
  "baseCommand": "touch",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/src",
      "inputBinding": {
        "position": 1,
        "valueFrom": "$(self.nameroot).class"
      },
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "#main/classfile",
      "outputBinding": {
        "glob": "*.class"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "InitialWorkDirRequirement",
      "listing": [
        "$(inputs.src)"
      ]
    }
  ]
}
