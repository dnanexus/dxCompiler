{
  "baseCommand": "wc",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "wc4-tool.cwl",
  "inputs": [
    {
      "id": "file1",
      "inputBinding": {},
      "type": "File"
    }
  ],
  "outputs": [
    {
      "id": "output",
      "outputBinding": {
        "glob": "output.txt",
        "loadContents": true,
        "outputEval": "${\n  var s = self[0].contents.split(/\\r?\\n/);\n  return parseInt(s[s.length-2]);\n}\n"
      },
      "type": "int"
    }
  ],
  "requirements": [
    {
      "class": "InlineJavascriptRequirement"
    }
  ],
  "stdout": "output.txt"
}
