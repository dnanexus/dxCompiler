{
  "baseCommand": "echo",
  "class": "CommandLineTool",
  "cwlVersion": "v1.2",
  "id": "#main",
  "inputs": [
    {
      "id": "#main/hello",
      "inputBinding": {
        "valueFrom": "$(self.a)/$(self.b)"
      },
      "type": "#main/HelloType"
    }
  ],
  "outputs": [
    {
      "id": "#main/output",
      "outputBinding": {
        "glob": "output.txt"
      },
      "type": "File"
    }
  ],
  "requirements": [
    {
      "class": "SchemaDefRequirement",
      "types": [
        {
          "fields": [
            {
              "name": "#main/HelloType/a",
              "type": "string"
            },
            {
              "name": "#main/HelloType/b",
              "type": "string"
            }
          ],
          "name": "#main/HelloType",
          "type": "record"
        }
      ]
    }
  ],
  "stdout": "output.txt"
}
