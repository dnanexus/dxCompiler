{
    "class": "CommandLineTool",
    "id": "#main",
    "label": "CLAMMS: Call CNVs for a sample using a pre-computed reference model",
    "requirements": [
        {
            "class": "DockerRequirement",
            "dockerOutputDirectory": "/data/out",
            "dockerPull": "pfda2dockstore/clamms_call_cnvs:12"
        }
    ],
    "hints": [
        {
            "loadListing": "deep_listing",
            "class": "LoadListingRequirement"
        },
        {
            "networkAccess": true,
            "class": "file:///Users/yshi/Documents/dnanexus/dxCompiler/test/cwl_selected_wf/clamms_call_cnvs/NetworkAcess"
        }
    ],
    "inputs": [
        {
            "default": 1e-07,
            "doc": "Reflects the expected rate of CNVs across the exome. Lower values increase sensitivity.",
            "inputBinding": {
                "position": 4,
                "prefix": "--cnv_rate"
            },
            "type": [
                "null",
                "double"
            ],
            "id": "#main/cnv_rate"
        },
        {
            "doc": "Pre-computed model file fit on a reference panel",
            "inputBinding": {
                "position": 2,
                "prefix": "--reference_model_file"
            },
            "type": "File",
            "id": "#main/reference_model_file"
        },
        {
            "doc": "BED file of pre-computed normalized coverage for the test sample",
            "inputBinding": {
                "position": 1,
                "prefix": "--sample_normalized_coverage_bed_file"
            },
            "type": "File",
            "id": "#main/sample_normalized_coverage_bed_file"
        },
        {
            "doc": "\"M\" or \"F\" for assigning expected ploidy on sex chromosomes. If not provided, CNVs will not be called on sex chormosomes.",
            "inputBinding": {
                "position": 3,
                "prefix": "--sex"
            },
            "type": [
                "null",
                "string"
            ],
            "id": "#main/sex"
        }
    ],
    "baseCommand": [],
    "s:author": {
        "class": "s:Person",
        "s:name": "Evan Maxwell"
    },
    "outputs": [
        {
            "doc": "BED file of CNVs called by CLAMMS on the test sample",
            "outputBinding": {
                "glob": "cnv_bed_file/*"
            },
            "type": "File",
            "id": "#main/cnv_bed_file"
        }
    ],
    "cwlVersion": "v1.2"
}