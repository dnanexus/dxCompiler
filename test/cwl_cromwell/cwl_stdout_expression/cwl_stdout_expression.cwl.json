{
    "id": "#main",
    "class": "CommandLineTool",
    "hints": [
        {
            "dockerPull": "debian:stretch-slim",
            "class": "DockerRequirement"
        },
        {
            "loadListing": "deep_listing",
            "class": "LoadListingRequirement"
        },
        {
            "networkAccess": true,
            "class": "NetworkAccess"
        }
    ],
    "inputs": [
        {
            "id": "#main/foo",
            "type": "string"
        },
        {
            "id": "#main/bar",
            "type": "string"
        }
    ],
    "stdout": "stdout-$(inputs.foo)-$(inputs.bar).txt",
    "baseCommand": [],
    "arguments": [
        "echo",
        "$(inputs.foo)",
        "$(inputs.bar)"
    ],
    "outputs": [
        {
            "type": "File",
            "id": "#main/b",
            "outputBinding": {
                "glob": "stdout-$(inputs.foo)-$(inputs.bar).txt"
            }
        }
    ],
    "cwlVersion": "v1.2"
}