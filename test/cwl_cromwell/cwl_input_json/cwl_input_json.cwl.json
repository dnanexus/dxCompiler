{
    "$graph": [
        {
            "id": "#main",
            "class": "Workflow",
            "inputs": [],
            "steps": [
                {
                    "id": "#main/make",
                    "run": "#makefile",
                    "in": [],
                    "out": [
                        "#main/make/fileoutput"
                    ]
                },
                {
                    "id": "#main/round",
                    "run": "#roundtrip",
                    "in": [
                        {
                            "source": "#main/make/fileoutput",
                            "id": "#main/round/input_record"
                        }
                    ],
                    "out": [
                        "#main/round/output_file"
                    ]
                }
            ],
            "outputs": [
                {
                    "type": "File",
                    "outputSource": "#main/round/output_file",
                    "id": "#main/final_output"
                }
            ]
        },
        {
            "id": "#makefile",
            "class": "CommandLineTool",
            "requirements": [
                {
                    "class": "ShellCommandRequirement"
                },
                {
                    "class": "InlineJavascriptRequirement"
                },
                {
                    "class": "DockerRequirement",
                    "dockerPull": "ubuntu:latest"
                }
            ],
            "inputs": [],
            "outputs": [
                {
                    "type": {
                        "fields": [
                            {
                                "name": "#makefile/fileoutput/input_record/input_file",
                                "type": "File"
                            }
                        ],
                        "name": "#makefile/fileoutput/input_record",
                        "type": "record"
                    },
                    "id": "#makefile/fileoutput"
                }
            ],
            "arguments": [
                {
                    "valueFrom": "echo foo > foo && echo '{ \"fileoutput\": { \"input_file\": {\"path\": \"$(runtime.outdir)/foo\", \"class\": \"File\"} } }' > cwl.output.json\n",
                    "shellQuote": false
                }
            ],
            "hints": [
                {
                    "loadListing": "deep_listing",
                    "class": "LoadListingRequirement"
                },
                {
                    "networkAccess": true,
                    "class": "NetworkAccess"
                }
            ]
        },
        {
            "id": "#roundtrip",
            "class": "CommandLineTool",
            "hints": [
                {
                    "class": "DockerRequirement",
                    "dockerPull": "stedolan/jq:latest"
                },
                {
                    "class": "NetworkAccess",
                    "networkAccess": true
                },
                {
                    "class": "LoadListingRequirement",
                    "loadListing": "deep_listing"
                }
            ],
            "inputs": [
                {
                    "id": "#roundtrip/input_record",
                    "type": {
                        "fields": [
                            {
                                "name": "#roundtrip/input_record/input_record/input_file",
                                "type": "File"
                            }
                        ],
                        "name": "#roundtrip/input_record/input_record",
                        "type": "record"
                    }
                }
            ],
            "outputs": [
                {
                    "id": "#roundtrip/output_file",
                    "type": "File"
                }
            ],
            "requirements": [
                {
                    "class": "ShellCommandRequirement"
                },
                {
                    "class": "InlineJavascriptRequirement"
                },
                {
                    "class": "InitialWorkDirRequirement",
                    "listing": [
                        {
                            "entry": "$(JSON.stringify(inputs))",
                            "entryname": "cwl.inputs.json"
                        }
                    ]
                }
            ],
            "arguments": [
                {
                    "valueFrom": "INPUT_FILE=\\$(cat cwl.inputs.json | jq -r '.. | .path? // empty') && ls $INPUT_FILE && echo \"{\\\"output_file\\\": {\\\"path\\\": \\\"\\$INPUT_FILE\\\", \\\"class\\\": \\\"File\\\"} }\" > cwl.output.json\n",
                    "shellQuote": false
                }
            ]
        }
    ],
    "cwlVersion": "v1.2"
}