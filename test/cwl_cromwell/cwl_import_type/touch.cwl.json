{
    "class": "CommandLineTool",
    "requirements": [
        {
            "class": "DockerRequirement",
            "dockerPull": "ubuntu:bionic-20180426"
        }
    ],
    "hints": [
        {
            "loadListing": "deep_listing",
            "class": "LoadListingRequirement"
        },
        {
            "networkAccess": true,
            "class": "NetworkAccess"
        }
    ],
    "inputs": [
        {
            "id": "#main/input",
            "type": "string",
            "inputBinding": {
                "position": 0
            }
        }
    ],
    "baseCommand": [
        "touch"
    ],
    "id": "#main",
    "outputs": [
        {
            "id": "#main/output",
            "type": "File",
            "outputBinding": {
                "glob": "$(inputs.input)"
            }
        }
    ],
    "cwlVersion": "v1.2"
}