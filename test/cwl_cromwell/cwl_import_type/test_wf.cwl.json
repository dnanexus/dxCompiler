{
    "$graph": [
        {
            "class": "Workflow",
            "requirements": [
                {
                    "class": "SchemaDefRequirement",
                    "types": [
                        {
                            "name": "#capture_kit.yml/capture_kit",
                            "type": "record",
                            "fields": [
                                {
                                    "name": "#capture_kit.yml/capture_kit/bait",
                                    "type": "string"
                                }
                            ]
                        }
                    ]
                }
            ],
            "inputs": [
                {
                    "id": "#main/bam",
                    "type": "string"
                },
                {
                    "id": "#main/capture_kit",
                    "type": "#capture_kit.yml/capture_kit"
                }
            ],
            "steps": [
                {
                    "id": "#main/touch_bam",
                    "run": "#touch.cwl",
                    "in": [
                        {
                            "id": "#main/touch_bam/input",
                            "source": "#main/bam"
                        }
                    ],
                    "out": [
                        {
                            "id": "#main/touch_bam/output"
                        }
                    ]
                }
            ],
            "id": "#main",
            "outputs": [
                {
                    "id": "#main/output_bam",
                    "type": "File",
                    "outputSource": "#main/touch_bam/output"
                }
            ]
        },
        {
            "class": "CommandLineTool",
            "requirements": [
                {
                    "class": "DockerRequirement",
                    "dockerPull": "ubuntu:bionic-20180426"
                }
            ],
            "hints": [
                {
                    "loadListing": "deep_listing",
                    "class": "LoadListingRequirement"
                },
                {
                    "networkAccess": true,
                    "class": "NetworkAccess"
                }
            ],
            "inputs": [
                {
                    "id": "#touch.cwl/input",
                    "type": "string",
                    "inputBinding": {
                        "position": 0
                    }
                }
            ],
            "baseCommand": [
                "touch"
            ],
            "outputs": [
                {
                    "id": "#touch.cwl/output",
                    "type": "File",
                    "outputBinding": {
                        "glob": "$(inputs.input)"
                    }
                }
            ],
            "id": "#touch.cwl"
        }
    ],
    "cwlVersion": "v1.2"
}