{
    "$graph": [
        {
            "class": "CommandLineTool",
            "requirements": [
                {
                    "class": "InlineJavascriptRequirement"
                }
            ],
            "hints": [
                {
                    "dockerPull": "debian:stretch-slim",
                    "class": "DockerRequirement"
                },
                {
                    "loadListing": "deep_listing",
                    "class": "LoadListingRequirement"
                },
                {
                    "networkAccess": true,
                    "class": "NetworkAccess"
                }
            ],
            "inputs": [],
            "baseCommand": [
                "touch",
                "z",
                "y",
                "x",
                "w",
                "c",
                "b",
                "a"
            ],
            "id": "#cwl_relative_imports_glob_sort.cwl",
            "outputs": [
                {
                    "type": "string",
                    "outputBinding": {
                        "glob": "?",
                        "outputEval": "${ return self.sort(function(a,b) { return a.location > b.location ? 1 : (a.location < b.location ? -1 : 0) }).map(f => f.basename).join(\" \") }\n"
                    },
                    "id": "#cwl_relative_imports_glob_sort.cwl/letters"
                }
            ]
        },
        {
            "id": "#main",
            "class": "Workflow",
            "inputs": [],
            "outputs": [
                {
                    "type": "string",
                    "outputSource": "#main/globSort/letters",
                    "id": "#main/letters"
                }
            ],
            "steps": [
                {
                    "in": [],
                    "run": "#cwl_relative_imports_glob_sort.cwl",
                    "out": [
                        "#main/globSort/letters"
                    ],
                    "id": "#main/globSort"
                }
            ]
        }
    ],
    "cwlVersion": "v1.2"
}