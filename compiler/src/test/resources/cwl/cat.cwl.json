{
    "class": "CommandLineTool",
    "doc": "Write a file to stdout using cat",
    "baseCommand": "cat",
    "stdout": "output.txt",
    "inputs": [
        {
            "type": "File",
            "inputBinding": {
                "position": 1
            },
            "id": "file"
        }
    ],
    "outputs": [
        {
            "type": "File",
            "id": "contents",
            "outputBinding": {
                "glob": "output.txt"
            }
        }
    ],
    "cwlVersion": "v1.2"
}