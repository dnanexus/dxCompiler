<%@ val command: String %>
<%@ val rtTraceLevel: Int %>
<%@ val streamFiles: dx.core.io.StreamFiles.StreamFiles %>
<%@ val bashDollar: String = "$" %>
main() {
    java -jar ${bashDollar}{DX_FS_ROOT}/dxWDL.jar workflow ${command} ${bashDollar}{HOME} -traceLevel ${rtTraceLevel} -streamFiles ${streamFiles.toString}
}